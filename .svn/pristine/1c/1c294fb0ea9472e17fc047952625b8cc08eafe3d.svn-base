<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<!-- UserMapper.xml -->

<mapper namespace="net.zhaoxuyang.blog.mapper.ArticleCloudMapper">
    <resultMap id="ArticleCloudMap" type="ArticleCloud">
        <id property="word" column="word"/>
        <result property="count" column="count"/>
    </resultMap>
    
    <select id="list" resultMap="ArticleCloudMap">
        
        SELECT * FROM article_cloud 
    </select>
    
    

</mapper>
